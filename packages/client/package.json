{
  "name": "@cuple/client",
  "version": "1.2.0",
  "description": "typescript-based RPC with REST compatibility",
  "scripts": {
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:node": "tsc -p tsconfig.node.json",
    "build": "turbo run build:browser build:node",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fxdave/cuple.git"
  },
  "keywords": [
    "rpc",
    "type",
    "sharing",
    "rest",
    "procedure",
    "frontend",
    "api"
  ],
  "main": "./dist/node/index.js",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "import": "./dist/browser/index.js"
      },
      "node": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.js",
        "default": "./dist/node/index.js"
      },
      "default": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.js"
      }
    }
  },
  "types": "./dist/node/index.d.ts",
  "author": "David Biro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fxdave/cuple/issues"
  },
  "homepage": "https://github.com/fxdave/cuple#readme"
}
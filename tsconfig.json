{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@cuple/client/*": ["./packages/client/src/*"],
      "@cuple/server/*": ["./packages/server/src/*"],
      "@cuple/openapi/*": ["./packages/openapi/src/*"],
      "@cuple/client": ["./packages/client/src/index.ts"],
      "@cuple/server": ["./packages/server/src/index.ts"],
      "@cuple/openapi": ["./packages/openapi/src/index.ts"]

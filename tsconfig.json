  },
  "references": [
    { "path": "./packages/client" },
    { "path": "./packages/server" },
    { "path": "./packages/openapi" },
    { "path": "./test" }
  ],
  "files": [],
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  }
}
